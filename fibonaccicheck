#include<stdio.h>
int main()
{
int w;
printf("enter a number you want to check ");
scanf("%d",&w);
int a[1000];
a[0]=0;
a[1]=1;
int i=1,j;
while(a[i]<=w)
{
i=i+1;
a[i]=a[i-1]+a[i-2];
}
for(j=0;j<=i;j++)
{
printf("%d ", a[j]);
}
if(a[j-2]==w)
printf(" it is fibonacci number");
else
printf(" not fibonacci");


}
OUTPUT:-
enter a number you want to check 5
0 1 1 2 3 5 8  it is fibonacci number
